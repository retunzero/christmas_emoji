<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dteam.emoji" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Christmas Emoji -The Emoji Quiz</name>
    <description>Keep Calm and play christmas emoji</description>
    <author email="retunzero@gmail.com">DTeam</author>
    <icon height="128" src="icon-128.png" width="128" />
    <icon height="57" src="icon-57.png" width="57" />
    <icon height="72" src="icon-72.png" width="72" />
    <icon height="76" src="icon-76.png" width="76" />
    <icon height="120" src="icon-120.png" width="120" />
    <icon height="152" src="icon-152.png" width="152" />
    <icon height="114" src="icon-114.png" width="114" />
    <icon height="144" src="icon-144.png" width="144" />
    <icon gap:role="default" height="256" src="icon-256.png" width="256" />
    <splash height="480" src="splash_320_480.png" width="320" />
    <splash height="960" src="splash_640_960.png" width="640" />
    <splash height="1136" src="splash_640_1136.png" width="640" />
    <splash height="1334" src="splash_750_1334.png" width="750" />
    <splash height="1024" src="splash_768_1024.png" width="768" />
    <splash height="2208" src="splash_1242_2208.png" width="1242" />
    <splash height="2048" src="splash_1536_2048.png" width="1536" />
    <splash height="2732" src="splash_2048_2732.png" width="2048" />
    <preference name="permissions" value="none" />
    <preference name="android-targetSdkVersion" value="21" />
    <feature name="http://api.phonegap.com/1.0/network" />
    <preference name="orientation" value="portrait" />
    <preference name="fullscreen" value="true" />
    <gap:config-file parent="UIStatusBarHidden" platform="ios">
        <true />
    </gap:config-file>
    <gap:config-file parent="UIViewControllerBasedStatusBarAppearance" platform="ios">
        <false />
    </gap:config-file>
    <preference name="deployment-target" value="8.0" />
    <preference name="android-minSdkVersion" value="19" />
    <plugin name="cordova-plugin-crosswalk-webview" source="npm" />
    <plugin name="cordova-plugin-wkwebview-engine" />
    <plugin name="cordova-plugin-file" />
    <plugin name="cordova-plugin-httpd" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <access origin="*" />
    <plugin name="cordova-plugin-inappbrowser" source="npm" />
    <gap:plugin name="cordova-plugin-ad-admob" source="npm" />
</widget>
